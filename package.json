  "dependencies": {
    "body-parser": "^1.20.3",
    "bootstrap": "^4.5.0",
    "dotenv": "^8.6.0",
    "express": "^4.21.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^5.6.0",
    "mongoose": "^8.8.3",
