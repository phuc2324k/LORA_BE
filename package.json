{
  "name": "backend",
  "version": "1.0.0",
  "description": "A E-Commerce website developed using ReactJS for the frontend, NodeJs for the backend, MongoDB as database.",
  "main": "index.js",
  "engines": {
    "node": "18.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "nodemon --watch backend --exec babel-node backend/server.js",
    "heroku-postbuild": "cross-env NPM_CONFIG_PRODUCTION=false npm install && cd frontend && npm install && npm run build && cd ..",
    "build": "cross-env NPM_CONFIG_PRODUCTION=false npm install && cd frontend && npm install && npm run build && cd .."
  },
